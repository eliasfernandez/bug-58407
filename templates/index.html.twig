{{ form_start(form) }}
    <div class="my-custom-class-for-errors">
        {{ form_errors(form) }}
    </div>

    <div class="row">
        <div class="col">
            {{ form_row(form.firstName) }}
        </div>
        <div class="col" id="some-custom-id">
            {{ form_row(form.email) }}
        </div>
        <div class="col" id="some-custom-id">
            {{ form_row(form.test) }}
        </div>
    </div>
{{ form_end(form) }}
